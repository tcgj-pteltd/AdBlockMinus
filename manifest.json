{
    "name": "AddAds",
    "description": "Extension that adds ads to your site for you!",
    "version": "1.0",
    "manifest_version": 3,
    "chrome_url_overrides": {
        "newtab": "app.html"
    },
    "background": {
        "persistent": true,
        "scripts": ["background.js"]
    },
    "web_accessible_resources": [
        "woolooloo.mp4"
    ],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css":["content.css"]
    }],
    "permissions": [
        "activeTab"
    ]
}
